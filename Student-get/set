public class Student {
	
	private String spnr; 
	private String sname;
	private String saddress;
	private String sphnr; 
	
		
		public Student (String spnr, String sname, String saddress, String sphnr) {
		
			this.spnr = spnr;
			this.sname = sname;
			this.saddress = saddress; 
			this.sphnr = sphnr;
			
			}

			public String getSpnr() {
				return spnr;
			}

			public void setSpnr(String spnr) {
				this.spnr = spnr;
			}

			public String getSname() {
				return sname;
			}

			public void setSname(String sname) {
				this.sname = sname;
			}

			public String getSaddress() {
				return saddress;
			}

			public void setSaddress(String saddress) {
				this.saddress = saddress;
			}

			public String getSphnr() {
				return sphnr;
			}

			public void setSphnr(String sphnr) {
				this.sphnr = sphnr;
			}
			
}
