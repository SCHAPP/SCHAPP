CREATE TABLE Student
(spnr VARCHAR(11) NOT NULL,
sname VARCHAR(20),
sadress VARCHAR(20),
sphnr VARCHAR (20),
CONSTRAINT STUDENT_PK PRIMARY KEY(spnr))

CREATE TABLE Course
(ccode VARCHAR(20) NOT NULL,
cname VARCHAR(20),
cinfo VARCHAR(500),
CONSTRAINT COURSE_PK PRIMARY KEY(ccode))

CREATE TABLE StudentRead
(spnr VARCHAR(11) NOT NULL,
ccode VARCHAR(20) NOT NULL,
grade VARCHAR (20),
CONSTRAINT STUDENTREAD_PK PRIMARY KEY(spnr,ccode),
CONSTRAINT STUDENTREAD_SPNR_FK FOREIGN KEY(spnr) REFERENCES Student(spnr),
CONSTRAINT STUDENTREAD_CCODE_FK FOREIGN KEY(ccode) REFERENCES Course(ccode))

CREATE TABLE Reading
(spnr VARCHAR(11) NOT NULL,
ccode VARCHAR(20) NOT NULL,
credit INTEGER,
CONSTRAINT READING_PK PRIMARY KEY(spnr,ccode),
CONSTRAINT READING_SPNR_FK FOREIGN KEY(spnr) REFERENCES Student(spnr),
CONSTRAINT READING_CCODE_FK FOREIGN KEY(ccode) REFERENCES Course(ccode))

