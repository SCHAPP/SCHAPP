package layout;

public class StudentRead {
	
	
	
	private Student spnr; 
	private Course ccode;
	private String grade;
	
	
	public StudentRead (Student spnr, Course ccode, String grade) {
		
		this.spnr= spnr;
		this.ccode= ccode;
		this.grade =grade; 
		
		
		}


	public Student getSpnr() {
		return spnr;
	}


	public void setSpnr(Student spnr) {
		this.spnr = spnr;
	}


	public Course getCcode() {
		return ccode;
	}


	public void setCcode(Course ccode) {
		this.ccode = ccode;
	}


	public String getGrade() {
		return grade;
	}


	public void setGrade(String grade) {
		this.grade = grade;
	}
	
	
	
	


}
