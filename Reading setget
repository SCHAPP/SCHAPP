package layout;

public class Reading {
	
	private Student spnr;
	private Course ccode;
	private int credit;
	
	
	
	public Reading (Student spnr, Course ccode, int credit) {
		
		this.spnr = spnr;
		this.ccode = ccode;
		this.credit = credit;
		
		
		}



	public Student getSpnr() {
		return spnr;
	}



	public void setSpnr(Student spnr) {
		this.spnr = spnr;
	}



	public Course getCcode() {
		return ccode;
	}



	public void setCcode(Course ccode) {
		this.ccode = ccode;
	}



	public int getCredit() {
		return credit;
	}



	public void setCredit(int credit) {
		this.credit = credit;


	}
}
